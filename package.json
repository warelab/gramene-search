{
  "name": "gramene-search",
  "version": "1.1.31",
  "description": "search wrapper for gramene",
  "scripts": {
    "build": "parcel build ./src/index.js",
    "watch": "parcel watch ./src/index.js",
    "build-maize": "SUBSITE=maize parcel build -d maize -o index.html src/maize.html",
    "build-grapevine": "SUBSITE=grapevine parcel build -d grapevine -o index.html src/grapevine.html",
    "build-main": "SUBSITE=main parcel build -d main -o index.html src/index.html",
    "build-sorghum": "SUBSITE=sorghum parcel build -d sorghum -o index.html src/sorghum.html",
    "build-rice": "SUBSITE=rice parcel build -d rice -o index.html src/rice.html",
    "start-maize": "SUBSITE=maize parcel src/maize.html",
    "start-grapevine": "SUBSITE=grapevine parcel  src/grapevine.html",
    "start-main": "SUBSITE=main parcel src/index.html",
    "start-sorghum": "SUBSITE=sorghum parcel src/sorghum.html",
    "start-rice": "SUBSITE=rice parcel src/rice.html",
    "prepare": "npm run build"
  },
  "author": "Andrew Olson",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "formik": "^3.0.0-next.8",
    "gramene-bins-client": "^2.2.14",
    "gramene-dbxrefs": "^3.0.4",
    "gramene-genetree-vis": "^4.1.7",
    "gramene-mdview": "^1.1.8",
    "gramene-search-vis": "^4.1.6",
    "gramene-taxonomy-with-genomes": "^3.0.10",
    "gramene-trees-client": "^2.5.6",
    "lodash": "^4.17.15",
    "lodash-compat": "^3.10.2",
    "money-clip": "^3.0.2",
    "querystringify": "^2.2.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-debounce-input": "^3.2.3",
    "react-dom": "17.0.2",
    "react-ga": "^3.3.0",
    "react-icons": "^4.1.0",
    "react-markdown": "^5.0.3",
    "react-router-dom": "^5.2.0",
    "react-tree-menu": "^1.5.0",
    "redux-bundler": "21.0.0",
    "redux-bundler-react": "1.0.1",
    "yup": "^0.32.9"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-less": "^2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/warelab/gramene-search.git"
  }
}
